{
  "name": "angular",
  "version": "0.0.1",
  "description": "Angular2 webpack boilerplate",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --config config/wp/wp.dev.js --profile --watch --inline",
    "dev:hmr": "npm run dev -- --hot",
    "build:dev": "webpack --config config/wp/wp.dev.js --progress --profile",
    "build": "webpack --config config/wp/wp.prod.js --progress --profile --bail",
    "watch": "npm run build:dev -- --watch",
    "watch:prod": "npm run build -- --watch",
    "test": "karma start"
  },
